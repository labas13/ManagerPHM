            <RowDefinition Height="30"/>
            <RowDefinition Height="25"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <Label Name="nazevOkna" Content="Přehled komodit" FontWeight="Bold" 
                FontSize="18" Background="#d7e4f2ff" Foreground="#135A95"/>
        <Menu Name="menuKomodit" Grid.Row="1" Background="#d7e4f2ff">
            <Label Name="label" Content="Komodita: "/>
            <ComboBox Name="komoditaCB" >

            </ComboBox>

        </Menu>
        <DataGrid Name="DGkomodity" Grid.Row="2">
            
        </DataGrid>
        <DockPanel Name="dokPanelStavApp" Background="#d7e4f2ff" Grid.Row="3">
            
        </DockPanel>
    </Grid>
</UserControl>
